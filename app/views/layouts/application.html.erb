<!DOCTYPE html>
<html>
<head>
  <script>document.documentElement.className = "js-enabled";</script>
  <%= render "shared/head", :title => Rails.configuration.app_title, :noapp => true %>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application" %>
</head>
<body class='<%= Rails.configuration.phase %>'>
  <%= render 'shared/header', :proposition => 'Repossess a property' %>
  <%= render 'shared/phase_indicator' %>
  <%= render 'layouts/header' %>
  
  <div id="wrapper">
    <main role="main">
      <%= render partial: 'shared/header_message' %>
      <%= render 'layouts/messages' %>
      <%= yield %>
      
    </main>
  </div>

  <%= render'shared/footer' %>

</body>
</html>
